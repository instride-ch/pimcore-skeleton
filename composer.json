{
  "name": "w-vision/pimcore-skeleton",
  "type": "project",
  "description": "The Pimcore skeleton for projects at w-vision.",
  "homepage": "https://w-vision.ch/",
  "license": "proprietary",
  "authors": [
    {
      "name": "w-vision AG",
      "email": "support@w-vision.ch",
      "homepage": "https://w-vision.ch/",
      "role": "Web Development Agency"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": ">=8.1",
    "dachcom-digital/emailizr": "^2.0",
    "dachcom-digital/seo": "^2.2",
    "dachcom-digital/toolbox": "^4.1",
    "pimcore/pimcore": "~10.6.0",
    "symfony/webpack-encore-bundle": "^2.1",
    "w-vision/pimcore-monitor-bundle": "^2.0"
  },
  "require-dev": {
    "deployer/deployer": "^7.3",
    "nelmio/alice": "^3.12",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-symfony": "^1.3",
    "symplify/easy-coding-standard": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Pimcore\\Model\\DataObject\\": "var/classes/DataObject"
    },
    "exclude-from-classmap": ["/vendor/vrana/adminer/plugins/drivers/"]
  },
  "scripts": {
    "post-create-project-cmd": "Pimcore\\Composer::postCreateProject",
    "post-install-cmd": [
      "Pimcore\\Composer::postInstall",
      "@pimcore-scripts"
    ],
    "post-update-cmd": [
      "Pimcore\\Composer::postUpdate",
      "@pimcore-scripts",
      "Pimcore\\Composer::executeMigrationsUp",
      "@pimcore-scripts"
    ],
    "pimcore-scripts": [
      "Pimcore\\Composer::clearCache",
      "Pimcore\\Composer::installAssets"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
